    - database/migrations/2025_01_11_194011_create_productions_table.php
    - database/migrations/2025_01_11_194012_create_detail_productions_table.php
    - database/migrations/2025_01_11_194013_create_sales_table.php
    - database/migrations/2025_01_11_194014_create_detail_sales_table.php

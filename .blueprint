    - database/migrations/2025_01_11_200525_create_productions_table.php
    - database/migrations/2025_01_11_200526_create_detail_productions_table.php
    - database/migrations/2025_01_11_200527_create_sales_table.php
    - database/migrations/2025_01_11_200528_create_detail_sales_table.php
